<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_user_mess_share"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.het.sdk.demo.ui.activity.share.UserMessShareActivity">

    <com.het.sdk.demo.widget.MultipleStatusView
        android:id="@+id/main_multiplestatusview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:contentView="@layout/message_list"
        app:emptyView="@layout/custom_empty_view"
        app:errorView="@layout/custom_error_view"
        app:loadingView="@layout/custom_loading_view"
        app:noNetworkView="@layout/custom_no_network_view">

    </com.het.sdk.demo.widget.MultipleStatusView>

</RelativeLayout>
