package com.het.open.lib.utils;

public class HetException extends RuntimeException {
	private static final long serialVersionUID = 475022994858770424L;

	public HetException() {
	}

	public HetException(String message) {
		super(message);
	}

	public HetException(String errorMsg, Throwable throwable) {
		super(errorMsg, throwable);
	}

	public HetException(Throwable throwable) {
		super(throwable);
	}
}
