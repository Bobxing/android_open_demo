package com.het.open.lib.model;

import java.io.Serializable;


/**
 * 设备大类型
 */
public class DeviceTypeModel implements Serializable{
	private static final long serialVersionUID = 1L;
	private String deviceTypeId;		//设备大分类标识
	private String deviceTypeName;		//设备大分类名称
	private String deviceTypeIcon;			//设备图标
	public String getDeviceTypeId() {
		return deviceTypeId;
	}
	public void setDeviceTypeId(String deviceTypeId) {
		this.deviceTypeId = deviceTypeId;
	}
	public String getDeviceTypeName() {
		return deviceTypeName;
	}
	public void setDeviceTypeName(String deviceTypeName) {
		this.deviceTypeName = deviceTypeName;
	}

	public String getDeviceTypeIcon() {
		return deviceTypeIcon;
	}

	public void setDeviceTypeIcon(String deviceTypeIcon) {
		this.deviceTypeIcon = deviceTypeIcon;
	}

	@Override
	public String toString() {
		return "DeviceTypeModel{" +
				"deviceTypeId='" + deviceTypeId + '\'' +
				", deviceTypeName='" + deviceTypeName + '\'' +
				", deviceTypeIcon='" + deviceTypeIcon + '\'' +
				'}';
	}
}
